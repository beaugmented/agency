{
	"name": "@beaugmented/agency",
	"version": "0.1.0",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beaugmented/agency.git",
		"directory": "packages/agency"
	},
	"packageManager": "pnpm@10.17.1",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": [
		"dist",
		"src",
		"CHANGELOG.md"
	],
	"scripts": {
		"build:js": "bun build --outdir dist --target node -- src/index.ts",
		"build:dts": "tsup",
		"build": "concurrently \"bun:build:*\"",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:once": "vitest run",
		"test:coverage": "echo no test coverage yet",
		"test:semver": "bun run build && bun run test:once"
	},
	"devDependencies": {
		"@types/node": "24.6.2",
		"@types/tmp": "0.2.6",
		"break-check": "0.6.16",
		"concurrently": "9.2.1",
		"eslint": "9.36.0",
		"openai": "6.0.1",
		"tmp": "0.2.5",
		"tsup": "8.5.0",
		"typescript": "5.9.3",
		"vite": "7.1.7",
		"vitest": "3.2.4",
		"zod": "4.1.11"
	},
	"dependencies": {
		"atom.io": "0.42.2",
		"varmint": "0.5.6"
	}
}
