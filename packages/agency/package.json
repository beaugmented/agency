{
	"name": "@beaugmented/agency",
	"version": "0.0.6",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beaugmented/agency.git",
		"directory": "packages/agency"
	},
	"packageManager": "pnpm@10.7.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": ["dist", "src", "CHANGELOG.md"],
	"scripts": {
		"build:js": "bun build --outdir dist --target node -- src/index.ts",
		"build:dts": "tsup",
		"build": "concurrently \"bun:build:*\"",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:once": "vitest run",
		"test:coverage": "echo no test coverage yet",
		"test:semver": "bun run build && bun run test:once"
	},
	"devDependencies": {
		"@types/node": "22.13.14",
		"@types/tmp": "0.2.6",
		"break-check": "0.6.8",
		"concurrently": "9.1.2",
		"openai": "4.90.0",
		"tmp": "0.2.3",
		"tsup": "8.4.0",
		"typescript": "5.8.2",
		"vite": "6.2.4",
		"vitest": "3.1.1",
		"zod": "3.24.2"
	},
	"dependencies": {
		"atom.io": "0.32.3",
		"varmint": "0.4.5"
	}
}
