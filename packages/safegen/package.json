{
	"name": "safegen",
	"version": "0.8.3",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beaugmented/agency.git",
		"directory": "packages/safegen"
	},
	"packageManager": "pnpm@10.28.2",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": [
		"dist",
		"src",
		"CHANGELOG.md"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./anthropic": {
			"types": "./dist/anthropic/index.d.ts",
			"import": "./dist/anthropic/index.js"
		},
		"./arktype": {
			"types": "./dist/arktype/index.d.ts",
			"import": "./dist/arktype/index.js"
		},
		"./google": {
			"types": "./dist/google/index.d.ts",
			"import": "./dist/google/index.js"
		},
		"./ollama": {
			"types": "./dist/ollama/index.d.ts",
			"import": "./dist/ollama/index.js"
		},
		"./openai": {
			"types": "./dist/openai/index.d.ts",
			"import": "./dist/openai/index.js"
		}
	},
	"scripts": {
		"build": "tsdown",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:once": "varmint track && vitest run && varmint clean --ci-flag=CI",
		"test:once:public": "vitest run public",
		"test:coverage": "echo no test coverage yet",
		"test:semver": "break-check --verbose"
	},
	"peerDependencies": {
		"@anthropic-ai/sdk": ">=0.60.0",
		"@google/genai": "^1.13.0",
		"arktype": ">=2.1.0",
		"ollama": ">=0.5.0",
		"openai": "^5.0.0 || ^6.0.0",
		"zod": ">=3.25.0"
	},
	"peerDependenciesMeta": {
		"@anthropic-ai/sdk": {
			"optional": true
		},
		"arktype": {
			"optional": true
		},
		"openai": {
			"optional": true
		},
		"ollama": {
			"optional": true
		},
		"zod": {
			"optional": true
		}
	},
	"dependencies": {
		"@standard-schema/spec": "1.1.0",
		"atom.io": "0.46.20",
		"varmint": "0.5.11"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "0.71.2",
		"@google/genai": "1.38.0",
		"@types/node": "24.10.9",
		"@types/tmp": "0.2.6",
		"break-check": "0.6.24",
		"eslint": "9.39.2",
		"ollama": "0.6.3",
		"openai": "6.16.0",
		"rimraf": "6.1.2",
		"tmp": "0.2.5",
		"tsdown": "0.20.1",
		"typescript": "5.9.3",
		"vite": "7.3.1",
		"vitest": "4.0.18",
		"zod": "4.3.6"
	}
}
