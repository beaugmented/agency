{
	"name": "safegen",
	"version": "0.2.11",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/beaugmented/agency.git",
		"directory": "packages/safegen"
	},
	"packageManager": "pnpm@10.3.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": ["dist", "src", "CHANGELOG.md"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./anthropic": {
			"types": "./dist/anthropic/index.d.ts",
			"import": "./dist/anthropic/index.js"
		},
		"./ollama": {
			"types": "./dist/ollama/index.d.ts",
			"import": "./dist/ollama/index.js"
		},
		"./openai": {
			"types": "./dist/openai/index.d.ts",
			"import": "./dist/openai/index.js"
		}
	},
	"scripts": {
		"build": "rimraf dist && tsup-node",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:once": "varmint -- track && vitest run; varmint --ci-flag=CI -- clean",
		"test:once:public": "vitest run public",
		"test:coverage": "echo no test coverage yet",
		"test:semver": "break-check --verbose"
	},
	"peerDependencies": {
		"@anthropic-ai/sdk": ">=0.32.0",
		"openai": ">=4.0.0",
		"zod": ">=3.0.0",
		"zod-to-json-schema": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"@anthropic-ai/sdk": {
			"optional": true
		},
		"openai": {
			"optional": true
		}
	},
	"dependencies": {
		"atom.io": "0.30.7",
		"varmint": "0.4.1"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "0.36.3",
		"@types/node": "22.13.2",
		"@types/tmp": "0.2.6",
		"break-check": "0.6.5",
		"ollama": "0.5.13",
		"openai": "4.84.0",
		"rimraf": "6.0.1",
		"tmp": "0.2.3",
		"tsup": "8.3.6",
		"typescript": "5.7.3",
		"vite": "6.1.0",
		"vitest": "3.0.5",
		"zod": "3.24.2",
		"zod-to-json-schema": "3.24.1"
	}
}
