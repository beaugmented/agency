{
	"name": "safegen",
	"version": "0.2.4",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@9.15.2",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"files": ["dist", "src", "CHANGELOG.md"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./anthropic": {
			"types": "./dist/anthropic/index.d.ts",
			"import": "./dist/anthropic/index.js"
		},
		"./ollama": {
			"types": "./dist/ollama/index.d.ts",
			"import": "./dist/ollama/index.js"
		},
		"./openai": {
			"types": "./dist/openai/index.d.ts",
			"import": "./dist/openai/index.js"
		}
	},
	"scripts": {
		"build": "rimraf dist && tsup-node",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint --flag unstable_ts_config -- .",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:once": "vitest run",
		"test:once:public": "vitest run public",
		"test:coverage": "echo no test coverage yet",
		"test:semver": "break-check --verbose"
	},
	"peerDependencies": {
		"@anthropic-ai/sdk": ">=0.32.0",
		"openai": ">=4.0.0",
		"zod": ">=3.0.0",
		"zod-to-json-schema": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"@anthropic-ai/sdk": {
			"optional": true
		},
		"openai": {
			"optional": true
		}
	},
	"dependencies": {
		"atom.io": "0.30.5",
		"varmint": "0.3.4"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "0.33.1",
		"@types/node": "22.10.2",
		"@types/tmp": "0.2.6",
		"break-check": "0.6.4",
		"ollama": "0.5.11",
		"openai": "4.77.0",
		"rimraf": "6.0.1",
		"tmp": "0.2.3",
		"tsup": "8.3.5",
		"typescript": "5.7.2",
		"vite": "6.0.6",
		"vitest": "2.1.8",
		"zod": "3.24.1",
		"zod-to-json-schema": "3.24.1"
	}
}
